		attribute<Units/Jaar> zichtjaar_jaar := zichtjaar/Jaar [ScenarioZichtjaarCombined/nr_2[nr_OrgEntity] ];
		attribute<String>     period_Name    := zichtjaar/period_Name[ScenarioZichtjaarCombined/nr_2[nr_OrgEntity] ];
		attribute<String>     Name           := Scenario_Name + '/'+ Zichtjaar_name;

		container V := for_each_nedv(name, 'value('+string(ID(.))+', ...)', void, .);
	}
