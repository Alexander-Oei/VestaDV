	unit<uint32> ScenarioZichtjaarCombined:  = combine(Scenario, zichtjaar);
	unit<uint32> ScenarioZichtjaar:          = Subset((ScenarioZichtjaarCombined/nr_1 == scenario/Huidig) == (ScenarioZichtjaarCombined/nr_2 == zichtjaar/J2006))
	{
		attribute<Bool> HasMutations:  = ScenarioZichtjaarCombined/nr_2[nr_OrgEntity] > Zichtjaar/j2010;
		
		attribute<String>     Scenario_Name  := Scenario/Name  [ScenarioZichtjaarCombined/nr_1[nr_OrgEntity] ];
